{
  "model": "tcn",
  "mode": "final",
  "train_roots": [
    "./DATA_EEG_TUH/tuh_eeg_seizure/v2.0.3/edf/train"
  ],
  "val_roots": [
    "./DATA_EEG_TUH/tuh_eeg_seizure/v2.0.3/edf/dev"
  ],
  "eval_roots": [
    "./DATA_EEG_TUH/tuh_eeg_seizure/v2.0.3/edf/eval"
  ],
  "max_records": 300,
  "max_records_train": 150,
  "max_records_val": 75,
  "max_records_eval": 75,
  "max_per_patient": 10,
  "include_background_only_records": true,
  "sampling_strategy": "none",
  "sampling_seed": 13,
  "condition": "filtered",
  "montage": "ar",
  "include_features": true,
  "selected_features": [
    "rms_eeg",
    "line_length",
    "tkeo_mean",
    "hjorth_activity",
    "hjorth_mobility",
    "hjorth_complexity",
    "bp_rel_delta",
    "bp_rel_theta",
    "bp_rel_alpha",
    "bp_rel_beta",
    "bp_rel_gamma",
    "spectral_entropy",
    "sef95",
    "beta_alpha_ratio",
    "theta_alpha_ratio",
    "std_eeg"
  ],
  "time_step_labels": false,
  "batch_size": 16,
  "epochs": 100,
  "max_training_minutes": 86400.0,
  "folds": 2,
  "num_filters": 64,
  "kernel_size": 9,
  "dropout": 0.2,
  "rnn_units": 64,
  "learning_rate": 0.0001,
  "optimizer": "adamw",
  "optimizer_weight_decay": 0.02,
  "optimizer_use_ema": true,
  "optimizer_ema_momentum": 0.995,
  "jit_compile": true,
  "use_class_weights": true,
  "loss_type": "focal",
  "focal_alpha": 0.25,
  "focal_gamma": 2.0,
  "tversky_alpha": 0.7,
  "tversky_beta": 0.3,
  "tversky_gamma": 1.333,
  "patience": 10,
  "min_lr": 1e-5,
  "lr_schedule_type": "cosine",
  "cosine_annealing_period": 4,
  "cosine_annealing_min_lr": 1e-5,
  "save_metric_checkpoints": true,
  "seed": 13,
  "dry_run": false,
  "output_dir": "./runs/tmp_features_tfrecord_test",
  "epoch_time_log_path": "./runs/tmp_features_tfrecord_test/epoch_times_custom.csv",
