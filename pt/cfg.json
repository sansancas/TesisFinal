  "tfrecord_dir": "./runs/tfrecord_exports",
  "tfrecord_compression": "GZIP",
  "dataset_storage": "auto",
  "dataset_auto_memmap_threshold_mb": 1024.0,
  "feature_worker_processes": 4,
  "feature_worker_chunk_size": 8,
  "feature_parallel_min_windows": 16,
  "dataset_force_memmap_after_build": true,
  "window_sec": 10.0,
  "hop_sec": 5.0,
  "epsilon": 1e-8,
  "target_fs": 256.0,
  "preprocess_bandpass": true,
  "preprocess_notch": true,
  "preprocess_normalize": true,
  "preprocess_n_harmonics": 2
